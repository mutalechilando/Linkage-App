#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@dhis2/cli-app-scripts/bin/d2-app-scripts" "$@"
  ret=$?
else 
  node  "$basedir/../@dhis2/cli-app-scripts/bin/d2-app-scripts" "$@"
  ret=$?
fi
exit $ret
